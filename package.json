{
    "name": "web-kernel",
    "version": "1.0.0",
    "description": "A kernel for web to handle scripting",
    "main": "dist/index.js",
    "types": "index.d.ts",
    "directories": {
        "example": "example"
    },
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production"
    },
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "laravel-mix": "^6.0.49",
        "ts-loader": "^9.5.4",
        "typescript": "^5.9.3"
    }
}
